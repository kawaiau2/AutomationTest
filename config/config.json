{
    "suite": "data/trialSuite.csv",
    "certificate":{
        "host": [
            "https://*",
            "https://*"
        ],
        "key": "./cert/xxx.key",
        "cert": "./cert/xxx.cer"
    },
    "env":[
        {
            "key":"xxxEndPoint",
            "value":"{{domain_uat_ec}}",
            "type":"any"
        },
        {
            "key":"xxxPCEndPoint",
            "value":"https://*",
            "type":"any"
        },
        {
            "key":"xxxCorpLink",
            "value":"UAT",
            "type":"any"
        }
    ],
    "updateJiraResult": true,
    "schemaCheck": true, 
    "log":{
        "request": false,
        "response": true,
        "schema": true,
        "console": false,
        "preRequest": false,
        "initEnv": false
    },
    "delay": {
        "instantWait": 500,
        "normalWait": 5000,
        "longWait": 120000,
        "pageLoad": 30000,
        "pollingWait": 1000 
    },
    "closeBrowserIfFailure": true,
    "browser": "Edge",
    "headless": false,
    "screenCap": true,
    "allStepScreenCap": true
}