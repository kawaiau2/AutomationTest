{
    "suite": "data/trialSuite.csv",
    "certificate":{
        "host": [
            "https://uat",
            "https://sit"
        ],
        "key": "./cert/sit",
        "cert": "./cert/sit"
    },
    "env":[
        {
            "key":"ConnectEndPoint",
            "value":"{{domain_uat}}",
            "type":"any"
        },
        {
            "key":"EndPoint",
            "value":"https://",
            "type":"any"
        },
        {
            "key":"CorpLink",
            "value":"UAT",
            "type":"any"
        }
    ],
    "updateJiraResult": false,
    "schemaCheck": false, 
    "log":{
