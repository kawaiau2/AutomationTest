{
    "suite": "data/trialSuite.csv",
    "certificate":{
        "host": [
            "https://www.makeplayingcards.com/",
            "https://www.boardgamesmaker.com/"
        ],
        "key": "./cert/*.key",
        "cert": "./cert/*.cer"
    },
    "env":[
        {
            "key":"boardGame",
            "value":"https://www.boardgamesmaker.com/",
            "type":"any"
        },
        {
            "key":"playingCard",
            "value":"https://www.makeplayingcards.com/",
            "type":"any"
        },
        {
            "key":"qpLink",
            "value":"{{boardGame}}",
            "type":"any"
        }
    ],
    "updateJiraResult": false,
    "schemaCheck": false, 
    "log":{
        "request": false,
        "response": false,
        "schema": false,
        "console": false,
        "logConsole": false,
        "preRequest": false,
        "initEnv": false
    },
    "delay": {
        "instantWait": 500,
        "normalWait": 5000,
        "longWait": 120000,
        "pageLoad": 30000,
        "pollingWait": 1000,
        "debugWait": 0,
        "caseInterval": 10
    },
    "closeBrowserIfFailure": false,
    "browser": {
        "browser": "Edge",
        "overRide": true
    },
    "headless": {
        "on": false,
        "new": true
    },
    "screenCap": true,
    "allStepScreenCap": false,
    "caseLog": {
        "log": true,
        "second": false,
        "third": false
    }
}